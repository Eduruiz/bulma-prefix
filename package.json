{
  "bundleDependencies": false,
  "deprecated": false,
  "name": "bulma-builder",
  "description": "A simple NPM builder for bulma.css with a custom class namespace",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "bulma": "^0.9.0",
    "bulma-checkradio": "^1.1.1",
    "clean-css-cli": "^4.3.0",
    "node-sass": "^4.13.1",
    "postcss": "^7.0.30",
    "postcss-cli": "^7.1.1",
    "postcss-prefixer": "^2.1.2",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "build": "npm run clean && npm run sass && npm run autoprefix && npm run cleancss && npm run prefix && npm run prefix-cleancss && npm run prefix-sourcemap",
    "clean": "rimraf all",
    "sass": "node-sass --output-style expanded --source-map true node_modules/bulma/bulma.sass all/bulma.css",
    "autoprefix": "postcss --use autoprefixer --map false --output all/bulma.css all/bulma.css --verbose",
    "cleancss": "cleancss -o all/bulma.min.css all/bulma.css",
    "prefix": "postcss --no-map --output all/bulma.prefixed.css all/bulma.css --verbose",
    "prefix-cleancss": "cleancss -o all/bulma.prefixed.min.css all/bulma.prefixed.css",
    "prefix-sourcemap": "postcss --use autoprefixer --map true --output all/bulma.prefixed.css all/bulma.prefixed.css --verbose"
  }
}
